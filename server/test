SELECT "Contests"."id", "Contests"."contestId", "Contests"."userId", "Contests"."contestType", "Contests"."title",
"Contests"."name", "Contests"."status", "Contests"."price", "Contests"."type", "Contests"."typeOfVenture",
"Contests"."whatVentureDoes", "Contests"."targetCustomers", "Contests"."style", "Contests"."file",
 COUNT("Entries"."id") AS "numberOfEntry", "User"."id" AS "User.id", "User"."displayName" AS "User.displayName",
 "User"."avatar" AS "User.avatar", "Entries"."id" AS "Entries.id" FROM "Contests" AS "Contests"
 LEFT OUTER JOIN "Users" AS "User" ON "Contests"."userId" = "User"."id"
 LEFT OUTER JOIN "Entries" AS "Entries" ON "Contests"."id" = "Entries"."contestId" WHERE "Contests"."id" = '54'
 GROUP BY "Contest"."id", "User"."id", "Entries"."id"